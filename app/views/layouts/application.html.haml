!!!
%html
  %head
    %meta{:content => "text/html; charset=UTF-8", "http-equiv" => "Content-Type"}/
    %title App
    = csrf_meta_tags
    = csp_meta_tag
    
    = stylesheet_pack_tag 'application', media: 'all', 'data-turbolinks-track': 'reload'
    = javascript_pack_tag 'application', 'data-turbolinks-track': 'reload'
    = javascript_include_tag 'https://maps.googleapis.com/maps/api/js?key='+Rails.application.credentials.google_maps_api_key

    -# = javascript_include_tag src: "https://maps.googleapis.com/maps/api/js?key=Rails.application.credentials.google_maps_api_key"
    -# %script{src: "//maps.google.com/maps/api/js?key=#{Rails.application.credentials.google_maps_api_key}"}
    %script{src:"//cdn.rawgit.com/mahnunchik/markerclustererplus/master/dist/markerclusterer.min.js"}
    %script{src: '//cdn.rawgit.com/printercu/google-maps-utility-library-v3-read-only/master/infobox/src/infobox_packed.js'},{type: 'text/javascript'}
    = yield(:head_tags)
  %body
    = yield
